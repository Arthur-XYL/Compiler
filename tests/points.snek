(fun (coor x y) 
    (if (isnum x)
        (if (isnum y)
            (tuple x y)
            (tuple x 0)
        )
        (if (isnum y)
            (tuple 0 y)
            (tuple 0 0)
        )
    )
)

(fun (coor_combine a b)
    (tuple (+ (index a 0) (index b 0)) (+ (index a 1) (index b 1)))
)

(let ((a (coor 4 10))(b (coor 2 2)))
    (block
        (print a)
        (print b)
        (print (coor_combine a b))
        (print (coor_combine (coor_combine a b) (tuple 4 8)))
    )
)